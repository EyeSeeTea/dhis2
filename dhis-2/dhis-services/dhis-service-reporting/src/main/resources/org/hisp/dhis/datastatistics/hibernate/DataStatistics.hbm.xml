<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
  [<!ENTITY identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm">]
  >

<hibernate-mapping>
  <class name="org.hisp.dhis.datastatistics.DataStatistics" table="datastatistics">
    <cache usage="read-write" />
    <id name="id" column="statisticsid">
      <generator class="native" />
    </id>
    <property name="numberOfChartViews" column="chartviews" type="int"/>
    <property name="numberOfMapViews" column="mapviews" type="int"/>
    <property name="numberOfReportTablesViews" column="reporttableviews" type="int"/>
    <property name="numberOfEventReportViews" column="eventreportviews" type="int"/>
    <property name="numberOfEventChartViews" column="eventchartviews" type="int"/>
    <property name="numberOfDashboardViews" column="dashboardviews" type="int"/>
    <property name="numberOfIndicatorsViews" column="indicatorviews" type="int"/>
    <property name="numberOfUsers" column="users" type="int"/>
    <property name="totalNumberOfViews" column="totalviews" type="int"/>
    <property name="averageNumberOfViews" column="averageviews" type="double"/>
    <property name="averageNumberOfSavedCharts" column="averagenumcharts" type="double"/>
    <property name="averageNumberOfSavedMaps" column="averagenummaps" type="double"/>
    <property name="averageNumberOfSavedReportTables" column="averagenumreporttables" type="double"/>
    <property name="averageNumberOfSavedEventReports" column="averagenumeventreports" type="double"/>
    <property name="averageNumberOfSavedEventCharts" column="averagenumeventcharts" type="double"/>
    <property name="averageNumberOfSavedDashboards" column="averagenumdashborards" type="double"/>
    <property name="averageNumberOfSavedIndicators" column="averagenumindicators" type="double"/>
    <property name="totalNumberOfUsers" column="totalnumusers" type="int"/>
    &identifiableProperties;

  </class>
</hibernate-mapping>