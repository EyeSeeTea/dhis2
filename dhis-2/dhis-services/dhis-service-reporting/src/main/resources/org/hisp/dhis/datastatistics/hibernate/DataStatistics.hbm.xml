<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
  [<!ENTITY identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm">]
  >

<hibernate-mapping>
  <class name="org.hisp.dhis.datastatistics.DataStatistics" table="datastatistics">
    <cache usage="read-write" />
    <id name="id" column="statisticsid">
      <generator class="native" />
    </id>
    &identifiableProperties;
    <property name="numberOfChartViews" column="chartviews" type="double"/>
    <property name="numberOfMapViews" column="mapviews" type="double"/>
    <property name="numberOfReportTablesViews" column="reporttableviews" type="double"/>
    <property name="numberOfEventReportViews" column="eventreportviews" type="double"/>
    <property name="numberOfEventChartViews" column="eventchartviews" type="double"/>
    <property name="numberOfDashboardViews" column="dashboardviews" type="double"/>
    <property name="numberOfIndicatorsViews" column="indicatorviews" type="double"/>
    <property name="numberOfActiveUsers" column="users" type="int"/>
    <property name="totalNumberOfViews" column="totalviews" type="double"/>
    <property name="averageNumberOfViews" column="average_number_of_views" type="double"/>
    <property name="numberOfSavedCharts" column="number_of_charts" type="double"/>
    <property name="numberOfSavedMaps" column="number_of_maps" type="double"/>
    <property name="numberOfSavedReportTables" column="number_of_reporttables" type="double"/>
    <property name="numberOfSavedEventReports" column="number_of_eventreports" type="double"/>
    <property name="numberOfSavedEventCharts" column="number_of_eventcharts" type="double"/>
    <property name="numberOfSavedDashboards" column="number_of_dashborards" type="double"/>
    <property name="numberOfSavedIndicators" column="number_of_indicators" type="double"/>
    <property name="totalNumberOfUsers" column="total_number_of_users" type="int"/>

  </class>
</hibernate-mapping>